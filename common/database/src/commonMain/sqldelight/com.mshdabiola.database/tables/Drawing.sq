CREATE TABLE IF NOT EXISTS drawingEntity (
   id INTEGER PRIMARY KEY AUTOINCREMENT,
   path TEXT NOT NULL
);


getById:
SELECT *
FROM drawingEntity
WHERE id = ?;

getAll:
SELECT *
FROM  drawingEntity;

insert:
INSERT INTO drawingEntity(path)
VALUES ?;

deleteByID:
DELETE FROM drawingEntity
WHERE id = ?;

update:
UPDATE drawingEntity
SET path = ?
WHERE id = ?;

