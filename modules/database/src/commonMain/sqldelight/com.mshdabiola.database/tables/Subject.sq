CREATE TABLE IF NOT EXISTS subjectEntity(
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL
);



getById:
SELECT *
FROM subjectEntity
WHERE id = ?;

getAll:
SELECT *
FROM  subjectEntity;

insert:
INSERT INTO subjectEntity(name)
VALUES ?;

insertOrReplace:
INSERT OR REPLACE INTO subjectEntity(id,name)
VALUES ?;

deleteByID:
DELETE FROM subjectEntity
WHERE id = ?;

update:
UPDATE subjectEntity
SET name = ?
WHERE id = ?;

deleteAll:
DELETE FROM subjectEntity;